{
    "name": "laminbarrow/covid-19-stories",
    "description": "Opensource app that gathers data from social media and shows what people say about thier experiences with Covid-19.",
    "type": "project",
    "authors": [
        {
            "name": "Lamin Barrow",
            "email": "laminbarrow@gmail.com"
        }
    ],
    "require": {
        "silverstripe/recipe-core": "^4.5",
        "abraham/twitteroauth": "^1.1"
    },
    "require-dev": {
        "lekoala/silverstripe-debugbar": "^2.0",
        "oddnoc/silverstripe-artefactcleaner": "^4.0",
        "silverleague/ideannotator": "dev-master"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/config.yml"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
